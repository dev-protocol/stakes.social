{
  "name": "stakes.social",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn --cwd packages/web start:prod",
    "dev": "yarn --cwd packages/web start:dev",
    "build": "lerna run build --include-dependencies",
    "lint": "eslint --fix -c ./.eslintrc.json './packages/**/*.{ts,tsx}'",
    "format": "prettier --write ./packages/web/public/locales/en/PossessionOutline.json",
    "test": "lerna run test",
    "clean": "lerna run clean && lerna clean --y",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/bl": "5.0.2",
    "@types/jest": "27.0.3",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "concurrently": "6.5.1",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "lerna": "4.0.0",
    "lint-staged": "12.1.3",
    "nodemon": "2.0.15",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "serve": "13.0.2",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.4",
    "wait-on": "6.0.0"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "*.{.ts,tsx}": [
      "eslint --fix -c ./.eslintrc.json"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "bl": "5.0.0",
    "ipfs-http-client": "49.0.4"
  }
}
